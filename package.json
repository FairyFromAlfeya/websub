{
  "name": "websub",
  "version": "0.7.0",
  "packageManager": "yarn@3.2.4",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "check": "tsc --noEmit --skipLibCheck",
    "lint": "prettier --check . && eslint src --ext ts",
    "format": "prettier --write . && eslint src --ext ts --fix",
    "fmt": "yarn run format",
    "test": "yarn run check && yarn run lint",
    "prepack": "yarn run test && yarn run build"
  },
  "author": "Jack Baron <jackmbaron@gmail.com> (https://www.jackbaron.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lolPants/websub.git"
  },
  "engines": {
    "node": ">=16.9.0"
  },
  "devDependencies": {
    "@lolpants/eslint-config": "^0.2.3",
    "@types/node": "^18.11.9",
    "eslint": "^8.26.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@sapphire/shapeshift": "^3.7.0",
    "@web3-storage/parse-link-header": "^3.1.0",
    "axios": "^1.1.3",
    "cheerio": "^1.0.0-rc.12",
    "eventemitter3": "^4.0.7",
    "http-status-codes": "^2.2.0"
  }
}
